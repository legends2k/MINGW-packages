+++ b/Makefile.pre.in
@@ -1320,6 +1320,7 @@ Python/dynload_win.o: $(srcdir)/Python/dynload_win.c Makefile
 Python/sysmodule.o: $(srcdir)/Python/sysmodule.c Makefile $(srcdir)/Include/pydtrace.h
